@PACKAGE_INIT@

set(TNL_LBM_PROJECT_NAME "@PROJECT_NAME@")

# CMake build configuration
set(TNL_LBM_BUILD_TYPE "@CMAKE_BUILD_TYPE@")

# Compiler configuration
